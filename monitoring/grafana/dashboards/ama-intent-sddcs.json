{
  "dashboard": {
    "id": null,
    "uid": "ama-intent-sddcs",
    "title": "AMA-Intent + SDDCS Production Dashboard",
    "tags": ["ama-intent", "sddcs", "production"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m"]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "enable": true,
          "iconColor": "green",
          "tags": ["deployment"]
        }
      ]
    },
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "type": "row",
        "title": "üìä System Overview"
      },
      {
        "id": 2,
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 1},
        "type": "stat",
        "title": "Total Requests (24h)",
        "targets": [
          {
            "expr": "sum(increase(http_requests_total[24h]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 100000, "color": "yellow"},
                {"value": 500000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 1},
        "type": "stat",
        "title": "Success Rate",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"2..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 6, "x": 12, "y": 1},
        "type": "stat",
        "title": "P95 Latency",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "decimals": 3,
            "thresholds": {
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.5, "color": "yellow"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 1},
        "type": "stat",
        "title": "Active Users",
        "targets": [
          {
            "expr": "count(count by (user_id) (http_requests_total{endpoint=\"/api/\"}[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 10,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 9},
        "type": "row",
        "title": "üß† SDDCS Metrics"
      },
      {
        "id": 11,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 10},
        "type": "graph",
        "title": "SDDCS Checkpoints Created",
        "targets": [
          {
            "expr": "rate(sddcs_checkpoints_created_total[5m])",
            "legendFormat": "Checkpoints/sec",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Rate"},
          {"format": "short"}
        ],
        "legend": {"show": true, "alignAsTable": true, "avg": true, "max": true}
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 10},
        "type": "gauge",
        "title": "SDDCS Validation Success Rate",
        "targets": [
          {
            "expr": "sddcs_validation_success_rate * 100",
            "refId": "A"
          }
        ],
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 85, "color": "yellow"},
                {"value": 95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 10},
        "type": "stat",
        "title": "Rolling JWT Refreshes (1h)",
        "targets": [
          {
            "expr": "increase(sddcs_jwt_rolling_refreshes_total[1h])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "textMode": "value_and_name",
          "colorMode": "value",
          "graphMode": "area"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 20,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 18},
        "type": "row",
        "title": "üîÑ Long Horizon Agent"
      },
      {
        "id": 21,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 19},
        "type": "graph",
        "title": "Agent Task Completion Time",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_task_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(agent_task_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Duration"},
          {"format": "short"}
        ]
      },
      {
        "id": 22,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 19},
        "type": "graph",
        "title": "Agent Steps Progress",
        "targets": [
          {
            "expr": "agent_current_step",
            "legendFormat": "Current Step",
            "refId": "A"
          },
          {
            "expr": "agent_total_steps",
            "legendFormat": "Total Steps",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Step Number"},
          {"format": "short"}
        ]
      },
      {
        "id": 30,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 27},
        "type": "row",
        "title": "üíæ Cache Performance"
      },
      {
        "id": 31,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 28},
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m]) / (rate(cache_hits_total[5m]) + rate(cache_misses_total[5m])) * 100",
            "legendFormat": "Hit Rate %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "Hit Rate", "max": 100, "min": 0},
          {"format": "short"}
        ]
      },
      {
        "id": 32,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 28},
        "type": "stat",
        "title": "SDDCS Cache Validations (5m)",
        "targets": [
          {
            "expr": "increase(sddcs_cache_validations_total[5m])",
            "refId": "A"
          }
        ],
        "options": {
          "reduceOptions": {"calcs": ["lastNotNull"]},
          "textMode": "value",
          "colorMode": "value"
        }
      },
      {
        "id": 33,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 28},
        "type": "stat",
        "title": "Context Size Avg (MB)",
        "targets": [
          {
            "expr": "avg(context_size_bytes) / 1024 / 1024",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "decmbytes",
            "decimals": 2
          }
        }
      },
      {
        "id": 40,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 36},
        "type": "row",
        "title": "üñ•Ô∏è System Resources"
      },
      {
        "id": 41,
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 37},
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"ama-dashboard\"}[5m]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percent", "label": "CPU Usage"},
          {"format": "short"}
        ]
      },
      {
        "id": 42,
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 37},
        "type": "graph",
        "title": "Memory Usage",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"ama-dashboard\"} / 1024 / 1024",
            "legendFormat": "RSS (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Memory"},
          {"format": "short"}
        ]
      },
      {
        "id": 43,
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 37},
        "type": "graph",
        "title": "Database Size",
        "targets": [
          {
            "expr": "database_size_bytes / 1024 / 1024",
            "legendFormat": "DB Size (MB)",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "decmbytes", "label": "Size"},
          {"format": "short"}
        ]
      },
      {
        "id": 50,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 45},
        "type": "row",
        "title": "üìà Request Analytics"
      },
      {
        "id": 51,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 46},
        "type": "graph",
        "title": "Requests by Endpoint",
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total[5m]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Requests/sec"},
          {"format": "short"}
        ],
        "legend": {
          "show": true,
          "alignAsTable": true,
          "avg": true,
          "current": true,
          "max": true,
          "sort": "current",
          "sortDesc": true
        }
      },
      {
        "id": 60,
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 54},
        "type": "row",
        "title": "‚ö†Ô∏è Errors & Alerts"
      },
      {
        "id": 61,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 55},
        "type": "graph",
        "title": "Error Rate by Status Code",
        "targets": [
          {
            "expr": "sum by (status) (rate(http_requests_total{status=~\"4..|5..\"}[5m]))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Errors/sec"},
          {"format": "short"}
        ],
        "alert": {
          "name": "High Error Rate",
          "conditions": [
            {
              "evaluator": {"params": [10], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "1m",
          "message": "Error rate exceeded 10 req/s",
          "noDataState": "no_data"
        }
      },
      {
        "id": 62,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 55},
        "type": "table",
        "title": "Recent Errors",
        "targets": [
          {
            "expr": "topk(10, sum by (endpoint, status) (increase(http_requests_total{status=~\"5..\"}[1h])))",
            "refId": "A",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true},
              "indexByName": {},
              "renameByName": {
                "endpoint": "Endpoint",
                "status": "Status Code",
                "Value": "Count (1h)"
              }
            }
          }
        ]
      }
    ]
  },
  "overwrite": true,
  "folderId": 0,
  "folderUid": ""
}