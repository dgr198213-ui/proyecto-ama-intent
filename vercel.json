{
  "version": 2,
  "name": "ama-intent-v3",
  "builds": [
    {
      "src": "asgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "asgi.py"
    }
  ],
  "env": {
    "HOST": "0.0.0.0",
    "PORT": "5001",
    "LOG_LEVEL": "INFO",
    "OLLAMA_MODEL": "llama3.1",
    "USE_SUPABASE": "true",
    "SUPABASE_URL": "@supabase_url",
    "SUPABASE_KEY": "@supabase_key"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.11"
    }
  }
}
